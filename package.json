{
  "name": "autohyperlink",
  "displayName": "AutoHyperlink",
  "description": "Auto generate hyperlink and title",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:autohyperlink.insert"
  ],
  "main": "./Extension/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autohyperlink.insert",
        "title": "Insert a new hyperlink"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "request": "^2.88.2"
  }
}